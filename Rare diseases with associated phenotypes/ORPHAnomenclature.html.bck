                        <script>
                         $(document).ready(function(){
                                $.getJSON("rdcode_packs.json",function(data){
                                        var packs_data = '';
                                        $.each(data, function(key,value){
                                                packs_data += '<tr>';
                                                packs_data += '<td>' + value.aLanguage + '</td>';
                                                packs_data += '<td><a href="' + value.anUrl +'" download target="_blank">' + value.aProduct + '</a></td>';
                                                packs_data += '<td>' + value.aSize + '</td>';
                                                packs_data += '</tr>';
                                        });
                                        $('#rdcode_packs_table').append(packs_data);
                                        });
                         });
                         </script>

